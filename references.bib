@article{palmer_microstructural_2021,
	title = {Microstructural development from 9 to 14 years: {Evidence} from the {ABCD} {Study}},
	volume = {53},
	issn = {1878-9293},
	shorttitle = {Microstructural development from 9 to 14 years},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC8671104/},
	doi = {10.1016/j.dcn.2021.101044},
	abstract = {During late childhood behavioral changes, such as increased risk-taking and emotional reactivity, have been associated with the maturation of cortico-cortico and cortico-subcortical circuits. Understanding microstructural changes in both white matter and subcortical regions may aid our understanding of how individual differences in these behaviors emerge. Restriction spectrum imaging (RSI) is a framework for modelling diffusion-weighted imaging that decomposes the diffusion signal from a voxel into hindered, restricted, and free compartments. This yields greater specificity than conventional methods of characterizing diffusion. Using RSI, we quantified voxelwise restricted diffusion across the brain and measured age associations in a large sample (n = 8086) from the Adolescent Brain and Cognitive Development (ABCD) study aged 9–14 years. Older participants showed a higher restricted signal fraction across the brain, with the largest associations in subcortical regions, particularly the basal ganglia and ventral diencephalon. Importantly, age associations varied with respect to the cytoarchitecture within white matter fiber tracts and subcortical structures, for example age associations differed across thalamic nuclei. This suggests that age-related changes may map onto specific cell populations or circuits and highlights the utility of voxelwise compared to ROI-wise analyses. Future analyses will aim to understand the relevance of this microstructural developmental for behavioral outcomes.},
	urldate = {2022-10-21},
	journal = {Developmental Cognitive Neuroscience},
	author = {Palmer, Clare E. and Pecheva, Diliana and Iversen, John R. and Hagler, Donald J. and Sugrue, Leo and Nedelec, Pierre and Fan, Chun Chieh and Thompson, Wesley K. and Jernigan, Terry L. and Dale, Anders M.},
	month = dec,
	year = {2021},
	pmid = {34896850},
	pmcid = {PMC8671104},
	pages = {101044},
	file = {PubMed Central Full Text PDF:C\:\\Users\\hanna\\Zotero\\storage\\UG5ZYR7D\\Palmer et al. - 2021 - Microstructural development from 9 to 14 years Ev.pdf:application/pdf},
}

@article{luo_dopaminergic_2016-1,
	title = {Dopaminergic {Neurons} and {Brain} {Reward} {Pathways}: {From} {Neurogenesis} to {Circuit} {Assembly}},
