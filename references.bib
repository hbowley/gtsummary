	pmcid = {PMC2799505},
	pages = {55--58},
	file = {Full Text:C\:\\Users\\hanna\\Zotero\\storage\\CVT6ZBIV\\Romanos et al. - 2010 - Structural abnormality of the substantia nigra in .pdf:application/pdf},
