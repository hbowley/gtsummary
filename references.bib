	journal = {Psychopharmacology},
	author = {Solowij, Nadia and Jones, Katy A. and Rozman, Megan E. and Davis, Sasha M. and Ciarrochi, Joseph and Heaven, Patrick C.L. and Pesa, Nicole and Lubman, Dan I. and Yücel, Murat},
