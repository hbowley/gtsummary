	file = {PubMed Central Full Text PDF:C\:\\Users\\hanna\\Zotero\\storage\\S7FKIYA4\\McCaffrey et al. - 2013 - A Tutorial on Propensity Score Estimation for Mult.pdf:application/pdf},
