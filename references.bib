	pmcid = {PMC6561344},
	keywords = {Male, Adolescent, Adolescent Development, Cannabis, Child, Female, Humans, Marijuana Use, Memory, Short-Term, Prospective Studies, Substance use, Age Factors, Cerebral Cortex, Follow-Up Studies, Magnetic Resonance Imaging, Neuropsychological Tests, Executive Function, Adolescent Behavior, Adolescence, Longitudinal, fMRI, Cognitive Dysfunction, Executive function, Functional Neuroimaging, Wechsler Scales, Working memory},
