	keywords = {Male, Adolescent, Child, Cognition, Female, Humans, Memory, Short-Term, Prospective Studies, Alcohol Drinking, Follow-Up Studies, Neuropsychological Tests, Marijuana Smoking, Marijuana Abuse},
	pages = {738--748},
