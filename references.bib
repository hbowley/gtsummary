	keywords = {Brain, Male, Adolescent, Adolescents, Female, Humans, Underage Drinking, Age Factors, Alcohol Abuse, Alcoholism, Brain Mapping, Cerebral Cortex, Cognitive Neuroscience, Culture, Follow-Up Studies, Internal-External Control, Machine Learning, Magnetic Resonance Imaging, Neuropsychological Tests, Predictive Value of Tests, Risk Factors, Sex Factors, Socioeconomic Factors, United States},
